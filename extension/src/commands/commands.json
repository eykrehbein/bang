{
	"cmds": [
		{
			"cmd": "!a",
			"name": "Amazon",
			"target": "https://www.amazon.{{tld}}",
			"target_s": "https://www.amazon.{{tld}}/s?k={{q}}"
		},
		{
			"cmd": "!w",
			"name": "Wikipedia",
			"target": "https://{{lang}}.wikipedia.org",
			"target_s": "https://{{lang}}.wikipedia.org/wiki/{{q}}"
		},
		{
			"cmd": "!gh",
			"name": "Github",
			"target": "https://github.com",
			"target_s": "https://github.com/search?q={{q}}"
		},
		{
			"cmd": "!so",
			"name": "StackOverflow",
			"target": "https://stackoverflow.com",
			"target_s": "https://stackoverflow.com/search?q={{q}}"
		},
		{
			"cmd": "!fb",
			"name": "Facebook",
			"target": "https://facebook.com",
			"target_s": "https://www.facebook.com/search/top/?q={{q}}"
		},
		{
			"cmd": "!tw",
			"name": "Twitter",
			"target": "https://twitter.com",
			"target_s": "https://twitter.com/search?q={{q}}"
		},
		{
			"cmd": "!li",
			"name": "LinkedIn",
			"target": "https://www.linkedin.com",
			"target_s": "https://www.linkedin.com/search/results/all/?keywords={{q}}"
		},
		{
			"cmd": "!yt",
			"name": "Youtube",
			"target": "https://www.youtube.{{tld}}",
			"target_s": "https://www.youtube.{{tld}}/results?search_query={{q}}"
		},
		{
			"cmd": "!gdocs",
			"name": "Google Docs",
			"target": "https://docs.google.com/document",
			"target_s": "https://docs.google.com/document/?q={{q}}"
		},
		{
			"cmd": "!gsheets",
			"name": "Google Spreadsheets",
			"target": "https://docs.google.com/spreadsheets",
			"target_s": "https://docs.google.com/spreadsheets/?q={{q}}"
		},
		{
			"cmd": "!gslides",
			"name": "Google Presentations (Slides)",
			"target": "https://docs.google.com/presentation",
			"target_s": "https://docs.google.com/presentation/?q={{q}}"
		},
		{
			"cmd": "!gdrive",
			"name": "Google Drive",
			"target": "https://drive.google.com/drive",
			"target_s": "https://drive.google.com/drive/search?q={{q}}"
		},
		{
			"cmd": "!gmail",
			"name": "Gmail",
			"target": "https://mail.google.com/mail",
			"target_s": "https://mail.google.com/mail/#search/{{q}}"
		},
		{
			"cmd": "!gcal",
			"name": "Google Calendar",
			"target": "https://calendar.google.com/calendar",
			"target_s": "https://calendar.google.com/calendar/r/search?q={{q}}"
		},
		{
			"cmd": "!gphotos",
			"name": "Google Photos",
			"target": "https://photos.google.com",
			"target_s": "https://photos.google.com/search/{{q}}"
		},
		{
			"cmd": "!gkeep",
			"name": "Google Keep",
			"target": "https://keep.google.com",
			"target_s": "https://keep.google.com/#search/text%253D{{q}}"
		}
	]
}
